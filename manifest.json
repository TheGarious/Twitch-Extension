{
 "manifest_version": 2,
 "name": "Twitch Extension",
 "author": "gary.houbre@gmail.com",
 "description": "Twitch Extension",
 "version": "1.0",

 "browser_action": {
  "default_popup": "extension.html"
 },
 "icons": {
  "64" : "img/icon_64.png"
},
"permissions" : [
    "notifications"
  ],
  "background": {
          "scripts": ["background.js"],
          "persistent": false
      }
}
